<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx/17.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.cybookfx.MenuController">
    <children>
        <Button fx:id="buttonUserConsult" text="User Consult" onAction="#onUserConsultButtonClick"/>
        <Button fx:id="buttonOverdueBook" text="Overdue Book" onAction="#onOverdueBookButtonClick"/>
        <Button fx:id="buttonSearchBook" text="Search Book" onAction="#onSearchBookButtonClick"/>
    </children>
    <padding>
        <Insets top="10" right="10" bottom="10" left="10"/>
    </padding>
</VBox>
